{
  "name": "vue-element-ts",
  "version": "0.1.0",
  "private": true,
  "author": "https://github.com/Carrie999",
  "homepage": "https://carrie999.github.io/vue-element-ts/build/",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "commit": "git-cz",
    "prettier": "prettier --config ./.prettierrc --write 'src/**/*.ts' 'src/**/*.vue' ",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@commitlint/cli": "^8.2.0",
    "@types/nprogress": "^0.2.0",
    "@vue/cli-plugin-typescript": "^3.12.0",
    "@vue/eslint-config-prettier": "^5.0.0",
    "@xunlei/vue-lazy-component": "^1.1.3",
    "axios": "^0.21.1",
    "commitizen": "^4.0.3",
    "core-js": "^2.6.5",
    "element-ui": "^2.12.0",
    "eslint-plugin-prettier": "^3.1.1",
    "fs": "0.0.1-security",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "path": "^0.12.7",
    "semver-regex": "^3.1.0",
    "util": "^0.12.1",
    "vue": "^2.6.10",
    "vue-class-component": "^7.1.0",
    "vue-property-decorator": "^8.2.2",
    "vue-router": "^3.1.3"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^8.2.0",
    "@vue/cli-plugin-babel": "^3.12.0",
    "@vue/cli-service": "^3.12.0",
    "babel-eslint": "^10.0.1",
    "cz-conventional-changelog": "^3.0.2",
    "husky": "^3.0.9",
    "node-sass": "^4.12.0",
    "prettier": "^1.18.2",
    "sass-loader": "^8.0.0",
    "ts-loader": "^6.2.0",
    "typescript": "^3.6.4",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier"
    ],
    "rules": {
      "no-console": "off",
      "arrow-parens": "off",
      "arrow-spacing": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
